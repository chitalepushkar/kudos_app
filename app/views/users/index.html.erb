<h4> Users </h4>
<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Username</th>
      <th scope="col">Password</th>
      <th scope="col">Organization ID</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user|%>
      <tr>
        <td><%= full_name(user) %></td>
        <td><%= user.username %></td>
        <td><%= user.password %></td>
        <td><%= user.organization_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>